  "main": "src/main.ts",
  "scripts": {
    "build": "NODE_ENV=production rspack build",
    "prestart": "pnpm run build",
    "start": "http-server dist",
    "dev": "NODE_ENV=development rspack serve",
    "check:types": "tsc --noEmit",
