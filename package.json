{
  "name": "nexckycort.github.io",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack/webpack.config.ts --env COMPILE=production",
    "prestart": "npm run build",
    "start": "http-server dist",
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.config.ts --env COMPILE=development --open",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write src",
    "lhci:ci": "lhci autorun --config=./lighthouserc-ci.js",
    "lhci": "lhci autorun --config=./lighthouserc.js",
    "deploy": "gh-pages -d dist",
    "prepare": "husky install"
  },
  "devDependencies": {
