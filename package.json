    "clean-terminal-webpack-plugin": "^3.0.0",
    "gh-pages": "6.1.1",
    "http-server": "14.1.1",
    "lefthook": "^1.6.1",
    "normalize.css": "^8.0.1",
    "react-refresh": "^0.14.0",
